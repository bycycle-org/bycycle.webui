{
  "name": "bycycle.webui",
  "version": "1.0.0a1",
  "private": true,
  "dependencies": {
    "autoprefixer": "^9.5.1",
    "axios": "^0.18.0",
    "core-js": "^3.0.1",
    "lodash": "^4.17.11",
    "material-design-icons": "^3.0.1",
    "node-sass-chokidar": "1.3.4",
    "npm-run-all": "^4.1.5",
    "ol": "^5.3.2",
    "postcss-cli": "^6.1.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^7.0.2",
    "react-scripts": "^3.0.0",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0"
  },
  "scripts": {
    "start": "npm-run-all -p css:start js:start",
    "build": "npm-run-all css:build:production js:build:production",
    "test": "react-scripts test --env=jsdom",
    "css:start": "npm-run-all css:build css:watch",
    "css:build": "npm run css:compile",
    "css:build:production": "npm-run-all css:compile:production css:autoprefix",
    "css:autoprefix": "postcss 'src/**/*.css' --replace --use autoprefixer",
    "css:compile": "node-sass-chokidar --source-map true src/ -o src/",
    "css:compile:production": "node-sass-chokidar src/ -o src/",
    "css:watch": "node-sass-chokidar src/ -o src/ --watch --recursive",
    "js:start": "react-scripts start",
    "js:build": "react-scripts build",
    "js:build:production": "react-scripts build"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "firefox esr",
    "not ie < 9"
  ]
}
